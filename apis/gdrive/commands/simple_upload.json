{
	 path_template: '/upload/drive/v2/files?uploadType=media',
	 verb: 'POST',
	 ret: 'title',
	 headers: {
	 	"Content-Type": 'text/plain',
	 	"Content-Length": '{length}'
	 },
	 body: '{text}',
	 options: [
	           {
	        	   short: 't',
	        	   long: 'text',
	        	   def: 'text to upload',
	        	   desc: 'text to upload',
	           },
	           {
	        	   short: 'l',
	        	   long: 'length',
	        	   def: 'length of text in bytes',
	        	   desc: 'length of text in bytes',
	           },
	           {
	        	   short: 'a',
	        	   long: 'access_token',
	        	   def: 'access token',
	        	   desc: 'access_token',
	           },
	           
	 ]
 }
			 